{
  "name": "gray-swatch",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "check:prettier": "prettier --check .",
    "check:lint": "eslint .",
    "check:ts": "vue-tsc --noEmit",
    "check:all": "pnpm check:prettier && pnpm check:lint && pnpm check:ts",
    "fix:prettier": "prettier --log-level warn --write .",
    "fix:lint": "pnpm check:lint --fix",
    "fix:all": "pnpm fix:prettier && pnpm fix:lint"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "color-convert": "3.1.2",
    "roboto-fontface": "0.10.0",
    "vue": "3.5.21",
    "vuetify": "3.10.0",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@types/color-convert": "2.0.4",
    "@types/node": "22.18.3",
    "@types/webfontloader": "1.6.38",
    "@vitejs/plugin-vue": "6.0.1",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-vue": "10.4.0",
    "globals": "16.4.0",
    "jiti": "2.5.1",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0",
    "vite": "7.1.5",
    "vite-plugin-vuetify": "2.1.2",
    "vue-tsc": "3.0.7"
  }
}
