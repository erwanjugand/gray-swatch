{
  "name": "gray-swatch",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "check:prettier": "prettier --check .",
    "check:lint": "eslint .",
    "check:ts": "vue-tsc --noEmit",
    "check:all": "pnpm check:prettier && pnpm check:lint && pnpm check:ts",
    "fix:prettier": "prettier --log-level warn --write .",
    "fix:lint": "pnpm check:lint --fix",
    "fix:all": "pnpm fix:prettier && pnpm fix:lint"
  },
  "dependencies": {
    "@erwanjugand/eslint-config": "1.1.0",
    "@mdi/font": "7.4.47",
    "color-convert": "3.1.3",
    "roboto-fontface": "0.10.0",
    "vue": "3.5.25",
    "vuetify": "3.11.2",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@types/color-convert": "2.0.4",
    "@types/node": "24.10.1",
    "@types/webfontloader": "1.6.38",
    "@vitejs/plugin-vue": "6.0.2",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "jiti": "2.6.1",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "vite": "7.2.6",
    "vite-plugin-vuetify": "2.1.2",
    "vue-tsc": "3.1.6"
  }
}
